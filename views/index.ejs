<html>
    <head>
        <link rel="stylesheet" type="text/css" href="/styles.css">
    </head>
    <body>
        <h1>Messages</h1>
        <a href="/new">Create new message</a>
        <div class="messagesContainer">
            <% for (let i = 0; i < messages.length; i++) { %>
                <div>
                    <p><%= messages[i].text %></p>
                    <a href="/message/<%= messages[i].id %>">message details</a>
                </div>
            <% } %>
        </div>
    </body>
</html>